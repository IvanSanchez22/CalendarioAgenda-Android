<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fivecalendar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="31" />

    <uses-feature android:name="android.hardware.screen.portrait" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/icono_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono_app_redondo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.FiveCalendar" >
        <activity
            android:name="com.example.fivecalendar.ClaseActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.fivecalendar.HorarioActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.fivecalendar.SemanaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.fivecalendar.TareaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.fivecalendar.NewTareaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.fivecalendar.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.FiveCalendar.Menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.fivecalendar.CalendarioActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.fivecalendar.DiaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>