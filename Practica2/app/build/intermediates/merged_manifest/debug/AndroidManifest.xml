<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practica2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="31" />

    <uses-feature android:name="android.hardware.screen.portrait" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/icono_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoredondo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.practica2" >
        <activity
            android:name="com.example.practica2.ReproductorMusica"
            android:exported="false" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.example.practica2.VerActivity"
            android:exported="false" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.example.practica2.ClaseActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.HorarioActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.AgendaProfesores"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.SemanaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.TareaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.NewTareaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.EditarAgenda"
            android:parentActivityName="com.example.practica2.ListaContactos" />
        <activity
            android:name="com.example.practica2.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.practica2.Menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.practica2.CalendarioActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.DiaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.practica2.ListaContactos"
            android:parentActivityName="com.example.practica2.MainActivity" />
    </application>

</manifest>